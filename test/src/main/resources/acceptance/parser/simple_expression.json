[
  {
    "name": "simple expressions",
    "cases": [
      {
        "input": "{{ 1+1 ",
        "expectedException": {
          "line": "1",
          "expected": "}}",
          "found": "EOF"
        }
      },
      {
        "input": "{{}}",
        "expectedException": {
          "line": "1",
          "expected": "}}",
          "found": "EOF"
        }
      },
      {
        "input": "{{*vjdsnlsnvs",
        "expectedException": {
          "line": "1",
          "expected": "*}}",
          "found": "EOF"
        }
      },
      {
        "input": "{{% {{5*10}} ",
        "expectedException": {
          "line": "1",
          "expected": "%}}",
          "found": "EOF"
        }
      },
      {
        "input": "{{ (5*7 }}",
        "expectedException": {
          "line": "1",
          "expected": ")",
          "found": "}}"
        }
      },
      {
        "input": "{{{{}}",
        "expectedException": {
          "line": "1",
          "expected": "}}",
          "found": "EOF"
        }
      },
      {
        "input": "a {{@ getDayOfWeek('a',3,1) -> isUndefined() b",
        "expectedException": {
          "line": "1",
          "expected": "}}",
          "found": "b"
        }
      }
    ]
  }
]