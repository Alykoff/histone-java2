[
  {
    "name": "expressions",
    "cases": [
      {
        "input": "{{}}",
        "expectedResult": ""
      },
      {
        "input": "a{{}}b",
        "expectedResult": "ab"
      },
      {
        "input": "a{{'a' + 'b'}}b",
        "expectedResult": "aabb"
      },
      {
        "input": "{{if 1 || 2 && 3 | 4 & 2}}{{/if}}",
        "expectedAST": "[29,[24,[29],[7,1,[6,2,[30,3,[32,4,2]]]]]]"
      },
      {
        "input": "{{% {{5*10}} %}}",
        "expectedResult": " {{5*10}} "
      },
      {
        "input": "{{ var x = {{%{{5+5}}%}}}}{{x}}",
        "expectedResult": "{{5+5}}"
      },
      {
        "input": "a {{@ getDayOfWeek('a',3,1) -> isUndefined()}} b",
        "expectedResult": "a  b"
      },
      {
        "input": "a {{var x = [1\r\n, 2\r\n, 3\r\n]}} b",
        "expectedResult": "a  b"
      },
      {
        "input": "a {{var x = [\r\n bla: 'dd', 2\r\n, 3\r\n]}} b",
        "expectedResult": "a  b"
      }
    ]
  }
]