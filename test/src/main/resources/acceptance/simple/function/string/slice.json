[
  {
    "name": "String -> slice",
    "cases": [
      {
        "input": "--{{'foobar'->slice('5', '3.0')}}",
        "expectedResult": "--r"
      },
      {
        "input": "--{{'foobar'->slice(2, 1)}}",
        "expectedResult": "--o"
      },
      {
        "input": "{{\"foobar\"->slice(0, -1)}}",
        "expectedResult": "fooba"
      },
      {
        "input": "{{'12345667'->slice('1',22)}}",
        "expectedResult": "2345667"
      },
      {
        "input": "{{'12345667'->slice('1')}}",
        "expectedResult": "2345667"
      }
    ]
  }
]