[
  {
    "name": "Number.toChar",
    "cases": [
      {
        "input": "<div>{{65->toChar}} = A</div>",
        "expectedResult": "<div>A = A</div>"
      },
      {
        "input": "<div>{{97->toChar}} = a</div>",
        "expectedResult": "<div>a = a</div>"
      },
      {
        "input": "<div>{{48->toChar}} = 0</div>",
        "expectedResult": "<div>0 = 0</div>"
      },
      {
        "input": "a {{65 -> toChar()}} b",
        "expectedResult": "a A b"
      },
      {
        "input": "a {{65 -> toChar() -> isString()}} b",
        "expectedResult": "a true b"
      },
      {
        "input": "a {{65 -> toChar()}} b",
        "expectedResult": "a A b"
      },
      {
        "input": "a {{65 -> toChar() -> isNumber()}} b",
        "expectedResult": "a false b"
      }
    ]
  }
]



