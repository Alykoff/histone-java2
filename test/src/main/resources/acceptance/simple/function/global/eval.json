[
  {
    "name": "global -> eval",
    "cases": [
      {
        "input": "{{eval('{{this->toJSON}}', 123)}}",
        "expectedResult": "123"
      },
      {
        "input": "{{eval('{{this->toJSON}}', [foo: 'bar'])}}",
        "expectedResult": "{\"foo\":\"bar\"}"
      },
      {
        "input": "{{eval('{{getBaseURI}}', null, 'x')}}",
        "expectedResult": "x"
      },
      {
        "input": "{{eval('{{getBaseURI}}', null, 123) != 'x'}}",
        "expectedResult": "true"
      },
      {
        "input": "{{eval('[29,[20,[22,[4],\"eval\",\"{{getBaseURI}}\",null,123],\"x\"]]', null, 123)}}",
        "expectedResult": "true"
      }
    ]
  }
]