[
  {
    "name": "string -> strip",
    "cases": [
      {
        "input": "{{'\t\n\r\r  \t\t\t\thistone     \t   \n'->strip}}",
        "expectedResult": "histone"
      },
      {
        "input": "{{'\t\n\r\r  \t    --\thistone  \ta \r\r  \n'->strip}}",
        "expectedResult": "--\thistone  \ta"
      },
      {
        "input": "{{'/X////foo////bar///X//'->strip('/X')}}",
        "expectedResult": "foo////bar"
      },
      {
        "input": "{{''->strip('/X')}}",
        "expectedResult": ""
      },
      {
        "input": "{{'aaaaaaa'->strip('a')}}",
        "expectedResult": ""
      },
      {
        "input": "{{'a'->strip('a')}}",
        "expectedResult": ""
      },
      {
        "input": "{{'    '->strip}}",
        "expectedResult": ""
      },
      {
        "input": "{{'   \t\n '->strip}}",
        "expectedResult": ""
      },
      {
        "input": "{{'\t\n'->strip}}",
        "expectedResult": ""
      },
      {
        "input": "{{'\t\nt\n'->strip}}",
        "expectedResult": "t"
      }
    ]
  }
]