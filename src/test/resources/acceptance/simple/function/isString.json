[
  {
    "name": "String.isString",
    "cases": [
      {
        "input": "<div>{{'string'->isString}} = true</div>",
        "expectedResult": "<div>true = true</div>"
      },
      {
        "input": "<div>{{''->isString}} = true</div>",
        "expectedResult": "<div>true = true</div>"
      },
      {
        "input": "<div>{{0->isString}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      },
      {
        "input": "<div>{{true->isString}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      },
      {
        "input": "<div>{{undefined->isString}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      },
      {
        "input": "<div>{{macro re(a)}}a{{/macro}}{{re->isString}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      },
      {
        "input": "<div>{{(-1)->isString}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      },
      {
        "input": "<div>{{[1, 2, 3]->isString}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      }
    ]
  }
]