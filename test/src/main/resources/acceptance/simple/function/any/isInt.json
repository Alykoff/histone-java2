[
  {
    "name": "type -> isInt",
    "cases": [
      {
        "input": "a {{10 -> isInt()}} b",
        "expectedResult": "a true b"
      },
      {
        "input": "a {{0.3 -> isInt()}} b",
        "expectedResult": "a false b"
      },
      {
        "input": "a {{(-3.33) -> isInt()}} b",
        "expectedResult": "a false b"
      },
      {
        "input": "<div>{{10->isInt}} = true</div>",
        "expectedResult": "<div>true = true</div>"
      },
      {
        "input": "<div>{{3.14->isInt}} = false</div>",
        "expectedResult": "<div>false = false</div>"
      },
      {
        "input": "<div>{{42.0->isInt}} = true</div>",
        "expectedResult": "<div>true = true</div>"
      }
    ]
  }
]