[
  {
    "name": "number -> toFloor",
    "cases": [
      {
        "input": "a {{0.60 -> toFloor()}} b",
        "expectedResult": "a 0 b"
      },
      {
        "input": "a {{0.40 -> toFloor()}} b",
        "expectedResult": "a 0 b"
      },
      {
        "input": "a {{5 -> toFloor()}} b",
        "expectedResult": "a 5 b"
      },
      {
        "input": "a {{5.1 -> toFloor()}} b",
        "expectedResult": "a 5 b"
      },
      {
        "input": "a {{(-5.9) -> toFloor()}} b",
        "expectedResult": "a -6 b"
      },
      {
        "input": "a {{(-5.9) -> toFloor() -> isNumber()}} b",
        "expectedResult": "a true b"
      }
    ]
  }
]