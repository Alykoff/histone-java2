[
  {
    "name": "arithmetic",
    "cases": [
      {
        "input": "{{'10' - 2}}",
        "expectedResult": "8"
      },
      {
        "input": "{{'x10' - 2}}",
        "expectedResult": ""
      },
      {
        "input": "{{'10' * 2}}",
        "expectedResult": "20"
      },
      {
        "input": "{{'x10' * 2}}",
        "expectedResult": ""
      },
      {
        "input": "{{'10' / 2}}",
        "expectedResult": "5"
      },
      {
        "input": "{{'x10' / 2}}",
        "expectedResult": ""
      },
      {
        "input": "{{'10' % 3}}",
        "expectedResult": "1"
      },
      {
        "input": "{{'x10' % 2}}",
        "expectedResult": ""
      },
      {
        "input": "{{-'x10'}}",
        "expectedResult": ""
      }
    ]
  },
  {
    "name": "addition",
    "cases": [
      {
        "input": "{{'10' + 2}}",
        "expectedResult": "102"
      },
      {
        "input": "{{10 + '-bar'}}",
        "expectedResult": "10-bar"
      },
      {
        "input": "{{'foo-' + 'bar'}}",
        "expectedResult": "foo-bar"
      },
      {
        "input": "{{'foo-' + true}}",
        "expectedResult": "foo-true"
      },
      {
        "input": "{{'foo-' + null}}",
        "expectedResult": "foo-null"
      },
      {
        "input": "{{[1, 2] + [3, 4]}}",
        "expectedResult": "1 2 3 4"
      },
      {
        "input": "{{[foo: 'bar'] + [x: 'yyy']}}",
        "expectedResult": "bar yyy"
      },
      {
        "input": "{{[1, 2] + [x: 'yyy']}}",
        "expectedResult": "1 2 yyy"
      },
      {
        "input": "{{[x: 'yyy'] + [1, 2]}}",
        "expectedResult": "yyy 1 2"
      },
      {
        "input": "{{10 / 0}}",
        "expectedResult": ""
      },
      {
        "input": "{{'string' * [foo: 'bar']}}",
        "expectedResult": ""
      },
      {
        "input": "{{true + 3.14}}",
        "expectedResult": ""
      },
      {
        "input": "{{0b10+0B1}}",
        "expectedResult": "3"
      },
      {
        "input": "{{0x22+0X33}}",
        "expectedResult": "85"
      },
      {
        "input": "--{{[1, 2, 3][0.0]}}",
        "expectedResult": "--1"
      },
      {
        "input": "-{{[1, 2, 3]['0.0']}}",
        "expectedResult": "-1"
      },
      {
        "input": "{{-'3'}}",
        "expectedResult": "-3"
      }
    ]
  }
]
